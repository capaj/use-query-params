{
  "name": "use-url-query",
  "version": "0.3.2",
  "description": "React Hook for managing state in URL query parameters",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "prepare": "npm run build",
    "build:esm": "tsc --module esnext --target esnext --outDir lib/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib/cjs",
    "build-website": "npm run test && npm run build && cd examples/website-example && npm run build && npm run copy-build",
    "dev": "NODE_ENV=development tsc -w",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest",
    "jw": "jest --watch",
    "jc": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pbeshai/use-query-params.git"
  },
  "keywords": [
    "react",
    "url",
    "query",
    "parameters",
    "hook",
    "hooks",
    "query param",
    "react use"
  ],
  "contributors": [
    "Peter Beshai <peter.beshai@gmail.com>",
    "Jiri Spac <capajj@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "babel-jest": "^24.8.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hooks-testing-library": "^0.5.1",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
